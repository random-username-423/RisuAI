{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": [
        "assets",
        "assets/*",
        "assets/**/*",
        "/assets/*",
        "/assets/**/*",
        "$APPDATA",
        "$APPDATA/*",
        "$APPDATA/**/*",
        "$DOWNLOAD/*",
        "/data/**/*",
        "$RESOURCE/*"
      ]
    },
    "window:allow-maximize",
    "window:allow-set-fullscreen",
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*/*"
        },
        {
          "url": "https://*/**/*"
        },
        {
          "url": "http://*/*"
        },
        {
          "url": "http://*/**/*"
        }
      ]
    },
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname",
    "process:allow-restart",
    "app:allow-app-show",
    "app:allow-app-hide",
    "process:default",
    "http:default",
    "os:default",
    "fs:default",
    "dialog:default",
    "shell:default"
  ]
}